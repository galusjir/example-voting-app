pipeline {
    agent any

    tools {
      nodejs 'NodeJS 8.9.0'
    }

    stages {
        stage('Build') {
            echo 'Compiling voting app'
            dir('result') {
                npm install
            }
        }

        stage('Test') {
            echo 'Testing voting app'
            dir('result') {
                npm install
                npm test
            }
        }

        
    }
}
